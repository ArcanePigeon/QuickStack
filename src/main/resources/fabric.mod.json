{
  "schemaVersion": 1,
  "id": "quickstack",
  "version": "${version}",

  "name": "Quick Stack [Fabric]",
  "description": "Automatically stack items into nearby chests",
  "authors": [
    "ArcanePigeon"
  ],
  "contact": {
    "homepage": "https://cloudwarp.org/",
    "sources": "https://github.com/ArcanePigeon/QuickStack.git"
  },
  "accessWidener": "quickstack.accesswidener",
  "license": "CC0-1.0",
  "icon": "assets/quickstack/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "org.cloudwarp.quickstack.QuickStack"
    ],
    "client": [
      "org.cloudwarp.quickstack.client.QuickStackClient"
    ],
    "modmenu": [
      "org.cloudwarp.quickstack.utils.QSModMenu"
    ]
  },
  "mixins": [
    "quickstack.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "minecraft": ">=1.19",
    "java": ">=17"
  }
}
